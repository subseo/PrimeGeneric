    "preview": "vite preview"
  },
  "dependencies": {
    "@types/node": "^20.12.2",
    "primevue": "^3.50.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-plugin-dts": "^3.8.1",
    "vue-tsc": "^2.0.7"
  }
}
