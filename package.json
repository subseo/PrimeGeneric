{
  "name": "prime-generic",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/prime-generic.umd.js",
  "module": "./dist/prime-generic.es.js",
  "types": "./dist/types/main.d.ts",
  "css": "global.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm build:css && pnpm build:types",
